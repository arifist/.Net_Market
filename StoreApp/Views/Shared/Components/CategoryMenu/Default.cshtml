@model IEnumerable<Category>
<div>
    <ul class="list-group">
        @foreach(var cat in Model)
        {
            <container class="list-group-item">

                <a asp-action="Index" asp-route-CategoryId="@cat.CategoryId">
                    @cat.CategoryName
                </a>
            </container>
        }
    </ul>
</div>